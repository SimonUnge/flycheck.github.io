<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
    <title>Flycheck â€” News</title>
    <link href="{{ site.url }}{{ site.baseurl }}/feed.atom" rel="self" />
    <link href="{{ site.url }}{{ site.baseurl }}" />
    <id>{{ site.url }}{{ site.baseurl }}/feed.atom</id>
    <author>
        <name>Sebastian Wiesner</name>
        <email>swiesner@lunaryorn.com</email>
    </author>
    <updated>{{ site.time | date_to_xmlschema }}</updated>
    {% for post in site.posts %}
    <entry>
      <title>{{ post.title | xml_escape }}</title>
      <link href="{{ site.url }}{{ site.baseurl }}{{ post.url }}" />
      <id>{{ site.url }}{{ site.baseurl }}{{ post.id }}</id>
      <published>{{ post.date | date_to_xmlschema }}</published>
      <summary type="html">{{ post.content | xml_escape }}</summary>
    </entry>
    {% endfor %}
</feed>
